{
  "version": "2.0.3",
  "compileOnSave": false,
  "compilerOptions": {
    "module": "esnext",
    "baseUrl": "./",
    "target": "es2015",
    "lib": [
      "es2017",
      "dom"
    ],
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "sourceMap": true,
    "inlineSources": true,
    "declaration": true,
    "removeComments": true,
    "traceResolution": true,
    "downlevelIteration": true,
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "moduleResolution": "node",
    "typeRoots": [
      "node_modules/@types"
    ],
    "paths": {
      "ng2-api-client": [
        "dist/ng2-api-client"
      ]
    }
  }
}
