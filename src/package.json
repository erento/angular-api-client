{
  "$schema": "../node_modules/ng-packagr/package.schema.json",
  "name": "ng2-api-client",
  "version": "6.1.0",
  "description": "Lazy loaded, responsive images and image backgrounds for Angular.",
  "author": "Erento GmbH <developers@erento.com> (https://www.erento.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/erento/angular-api-client.git"
  },
  "keywords": [
    "angular",
    "angular2",
    "api",
    "command",
    "client",
    "http",
    "ng"
  ],
  "license": "MIT",
  "homepage": "https://github.com/erento/angular-api-client",
  "peerDependencies": {
    "@angular/core": ">=6.0.0-rc.1",
    "@angular/common": ">=6.0.0-rc.1",
    "rxjs": ">=6.0.0-beta.4",
    "typescript": ">=2.5.0"
  },
  "ngPackage": {
    "lib": {
      "umdModuleIds": {
        "rxjs": "rxjs"
      },
      "entryFile": "public_api.ts"
    },
    "dest": "../dist"
  }
}
