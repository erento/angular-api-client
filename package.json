{
  "name": "ng2-api-client",
  "version": "0.0.0",
  "description": "API client for Angular",
  "files": [
    "src",
    "dist/*.js",
    "index.d.ts",
    "tsconfig.json"
  ],
  "main": "dist/angular-api-client.umd.js",
  "scripts": {
    "build": "npm run lint && npm run packagr",
    "karma": "node_modules/.bin/karma start",
    "lint": "tslint -p ./tsconfig.json '{src,test}/**/*.ts' -e '**/*.d.ts'",
    "packagr": "ng-packagr",
    "publish": "false",
    "rollup": "./node_modules/.bin/rollup -c -o dist/angular-api-client.umd.js",
    "postrollup": "./node_modules/.bin/uglifyjs ./dist/angular-api-client.umd.js > ./dist/angular-api-client.umd.min.js",
    "test": "npm run karma"
  },
  "author": "Erento GmbH <developers@erento.com> (https://www.erento.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/erento/angular-api-client.git"
  },
  "keywords": [
    "angular",
    "angular2",
    "api",
    "command",
    "client",
    "http",
    "ng"
  ],
  "license": "MIT",
  "devDependencies": {
    "@angular/common": "5.1.2",
    "@angular/compiler": "5.1.2",
    "@angular/compiler-cli": "5.1.2",
    "@angular/core": "5.1.2",
    "@angular/http": "5.1.2",
    "@angular/platform-browser": "5.1.2",
    "@angular/platform-browser-dynamic": "5.1.2",
    "@types/core-js": "0.9.43",
    "@types/jasmine": "2.8.3",
    "codelyzer": "4.0.2",
    "core-js": "2.5.3",
    "jasmine-core": "2.8.0",
    "karma": "1.7.1",
    "karma-jasmine": "1.1.1",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "2.0.9",
    "ng-packagr": "2.0.0-rc.4",
    "phantomjs-prebuilt": "2.1.16",
    "rxjs": "5.5.6",
    "ts-loader": "3.2.0",
    "tslint": "5.8.0",
    "tslint-eslint-rules": "4.1.1",
    "typescript": "2.5.3",
    "webpack": "1.13.3",
    "zone.js": "0.8.19"
  },
  "dependencies": {}
}
