{
  "name": "ng2-api-client",
  "version": "0.0.0",
  "description": "API client for Angular",
  "files": [
    "src",
    "dist/*.js",
    "index.d.ts",
    "tsconfig.json"
  ],
  "main": "dist/angular-api-client.umd.js",
  "scripts": {
    "build": "npm run lint && npm run test && npm run packagr",
    "karma": "node_modules/.bin/karma start",
    "lint": "tslint -p ./tsconfig.json '{src,test}/**/*.ts' -e '**/*.d.ts'",
    "packagr": "ng-packagr -p src/package.json",
    "publish": "false",
    "rollup": "./node_modules/.bin/rollup -c -o dist/angular-api-client.umd.js",
    "postrollup": "./node_modules/.bin/uglifyjs ./dist/angular-api-client.umd.js > ./dist/angular-api-client.umd.min.js",
    "test": "npm run karma"
  },
  "author": "Erento GmbH <developers@erento.com> (https://www.erento.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/erento/angular-api-client.git"
  },
  "keywords": [
    "angular",
    "angular2",
    "api",
    "command",
    "client",
    "http",
    "ng"
  ],
  "license": "MIT",
  "devDependencies": {
    "@angular/common": ">=6.0.0-rc.1",
    "@angular/compiler": ">=6.0.0-rc.1",
    "@angular/compiler-cli": ">=6.0.0-rc.1",
    "@angular/core": ">=6.0.0-rc.1",
    "@angular/http": ">=6.0.0-rc.1",
    "@angular/platform-browser": ">=6.0.0-rc.1",
    "@angular/platform-browser-dynamic": ">=6.0.0-rc.1",
    "@types/core-js": "0.9.46",
    "@types/jasmine": "2.8.6",
    "codelyzer": "4.2.1",
    "core-js": "2.5.3",
    "jasmine-core": "2.8.0",
    "karma": "2.0.0",
    "karma-jasmine": "1.1.1",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "2.0.9",
    "ng-packagr": "2.4.1",
    "phantomjs-prebuilt": "2.1.16",
    "rxjs": ">=6.0.0-beta.4",
    "ts-loader": "3.4.0",
    "tsickle": "0.27.2",
    "tslint": "5.9.1",
    "tslint-eslint-rules": "4.1.1",
    "typescript": "2.7.2",
    "webpack": "1.15.0",
    "zone.js": "0.8.20"
  },
  "dependencies": {}
}
