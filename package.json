{
  "name": "ng2-api-client",
  "version": "4.0.0",
  "description": "API client for Angular",
  "files": [
    "src",
    "dist/*.js",
    "index.d.ts",
    "tsconfig.json"
  ],
  "main": "dist/angular-api-client.umd.js",
  "scripts": {
    "build": "npm run lint && npm run tsc && npm run rollup",
    "karma": "node_modules/.bin/karma start",
    "lint": "tslint -p ./tsconfig.json '{src,test}/**/*.ts' -e '**/*.d.ts'",
    "prepublish": "npm run build",
    "rollup": "./node_modules/.bin/rollup -c -o dist/angular-api-client.umd.js",
    "postrollup": "./node_modules/.bin/uglifyjs ./dist/angular-api-client.umd.js > ./dist/angular-api-client.umd.min.js",
    "test": "npm run karma",
    "tsc": "node_modules/.bin/tsc -p ."
  },
  "author": "Erento GmbH <developers@erento.com> (https://www.erento.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/erento/angular-api-client.git"
  },
  "keywords": [
    "angular",
    "angular2",
    "api",
    "command",
    "client",
    "http",
    "ng"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@angular/core": ">=5.0.0",
    "@angular/common": ">=5.0.0",
    "rxjs": ">=5.5.2",
    "typscript": ">=2.4.1"
  },
  "devDependencies": {
    "@angular/common": "~5.0.0",
    "@angular/compiler": "~5.0.0",
    "@angular/core": "~5.0.0",
    "@angular/http": "~5.0.0",
    "@angular/platform-browser": "~5.0.0",
    "@angular/platform-browser-dynamic": "~5.0.0",
    "@types/core-js": "~0.9.43",
    "@types/jasmine": "~2.6.2",
    "codelyzer": "~4.0.0",
    "core-js": "~2.5.1",
    "jasmine-core": "~2.8.0",
    "karma": "~1.7.1",
    "karma-jasmine": "~1.1.0",
    "karma-phantomjs-launcher": "~1.0.2",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-spec-reporter": "~0.0.31",
    "karma-webpack": "~2.0.5",
    "phantomjs-prebuilt": "~2.1.15",
    "rollup": "~0.50.0",
    "rollup-plugin-typescript": "~0.8.1",
    "rxjs": "~5.5.2",
    "ts-loader": "~2.3.2",
    "tslint": "~5.8.0",
    "tslint-eslint-rules": "~4.1.1",
    "typescript": "~2.5.2",
    "uglify-js": "~3.1.6",
    "webpack": "~1.13.3",
    "zone.js": "~0.8.18"
  },
  "dependencies": {}
}
